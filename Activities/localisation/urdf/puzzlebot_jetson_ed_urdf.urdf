<?xml version="1.0"?>
<robot name="Puzzlebot Jetson-Lidar Ed.">


<!-- Dummy link to fiz robot chassis -->
<link name="base_link">

</link>


<link name="chassis">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.01" />
            <inertia ixx="7.58333333333e-06" ixy="0.0" ixz="0.0" iyy="7.58333333333e-06" iyz="0.0" izz="1.25e-05"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://localisation/models/MCR2_1000_13_Chassis.stl"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.57079632679 0 1.57079632679" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://localisation/models/MCR2_1000_13_Chassis.stl"/>
            </geometry>
        </visual>
</link>
<joint name="chassis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis"/>
    <origin xyz="0.0 0.0 0.05"/>
</joint>

<!-- FINISH THE URDF-->



<link name="left_wheel">
    <visual>
        <origin rpy="1 0 0" xyz="0 0 2"/>
        <geometry>
            <mesh filename="package://localisation/models/MCR2_1000_1_1_Wheel_Coupler_2.stl" scale="0.005 0.005 0.005"/>
        </geometry>
    </visual>
    
    <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <mesh filename="package://localisation/models/MCR2_1000_1_1_Wheel_Coupler_2.stl" scale="0.005 0.005 0.005"/>
        </geometry>
    </collision>
</link>
<joint name="wheel_rear_right_joint" type="continuous">
    <origin xyz="0.250 0.40 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="left_wheel"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>



</robot>
