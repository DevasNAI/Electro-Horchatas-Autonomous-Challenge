<?xml version="1.0"?>
<package format="2">
  <name>ros_deep_learning</name>
  <version>0.0.0</version>
  <description>Deep-learning nodes for ROS with support for NVIDIA Jetson and TensorRT, based on the jetson-inference library</description>
  <maintainer email="dustinf@nvidia.com">Dustin Franklin</maintainer>
  <license>Open</license>

  <!-- Build tool dependency -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Build dependencies -->
  <build_depend>cv_bridge</build_depend>
  <build_depend>image_transport</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>nodelet</build_depend>
  <build_depend>message_generation</build_depend>
  <build_depend>nav_2d_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>

  <!-- Execution dependencies -->
  <exec_depend>cv_bridge</exec_depend>
  <exec_depend>image_transport</exec_depend>
  <exec_depend>roscpp</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>nodelet</exec_depend>
  <exec_depend>message_runtime</exec_depend>
  <exec_depend>message_generation</exec_depend>
  <exec_depend>nav_2d_msgs</exec_depend>
  <exec_depend>geometry_msgs</exec_depend>

  <export>
    <nodelet plugin="${prefix}/ros_deep_learning_nodelets.xml"/>
  </export>
</package>
